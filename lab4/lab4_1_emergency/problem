(defproblem problem EMERGENCY
    (
        (helicopter supercopter)
        (empty supercopter)

        (crate_type food)
        (crate_type drinks)

        (location depot)
        (location location1)
        (location location2)
        (location location3)
        (location location4)
        (location location5)
        (location location6)

        (victim alice)
        (victim bob)
        (victim charlie)
        (victim david)
        (victim erin)
        (victim frank)
        (victim zoe)

        (crate crate01)
        (crate crate02)
        (crate crate03)
        (crate crate04)
        (crate crate05)
        (crate crate06)
        (crate crate07)
        (crate crate08)
        (crate crate09)
        (crate crate10)
        (of_type crate01 food)
        (of_type crate02 food)
        (of_type crate03 food)
        (of_type crate04 food)
        (of_type crate05 food)
        (of_type crate06 drinks)
        (of_type crate07 drinks)
        (of_type crate08 drinks)
        (of_type crate09 drinks)
        (of_type crate10 drinks)
        (can_be_picked_up crate01)
        (can_be_picked_up crate02)
        (can_be_picked_up crate03)
        (can_be_picked_up crate04)
        (can_be_picked_up crate05)
        (can_be_picked_up crate06)
        (can_be_picked_up crate07)
        (can_be_picked_up crate08)
        (can_be_picked_up crate09)
        (can_be_picked_up crate10)

        (at supercopter depot)
        (at crate01 depot)
        (at crate02 depot)
        (at crate03 depot)
        (at crate04 depot)
        (at crate05 depot)
        (at crate06 depot)
        (at crate07 depot)
        (at crate08 depot)
        (at crate09 depot)
        (at crate10 depot)
        (at alice location1)
        (at bob location2)
        (at charlie location1)
        (at david location3)
        (at erin location4)
        (at frank location5)

        (needs alice food)
        (needs alice drinks)
        (needs bob food)
        (needs charlie drinks)
        (needs david food)
        (needs erin food)
        (needs erin drinks)
        (needs frank drinks)
    )

    (
        (help_everyone supercopter)
    )
)
